{
  "name": "readGit",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.1.5",
    "jest-expo": "~1.0.1",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:install": "adb install -r app/build/outputs/apk/app-release.apk",
    "android:launch": "adb shell am start -a android.intent.action.MAIN -n com.readgit/.MainActivity",
    "android:sign": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore android/app/release.keystore android/app/build/outputs/apk/app-release-unsigned.apk $MYAPP_RELEASE_KEY_ALIAS --storepass $MYAPP_RELEASE_STORE_PASSWORD --keypass $MYAPP_RELEASE_KEY_PASSWORD",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js",
    "flow": "flow",
    "postinstall": "flow-typed update"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "color": "^1.0.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "next-frame": "^0.2.3",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-apollo": "^1.4.8",
    "react-native": "0.46.0",
    "react-native-animatable": "^1.2.2",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "reduxsauce": "^0.5.0",
    "seamless-immutable": "^7.1.2",
    "styled-components": "^2.1.1"
  }
}
