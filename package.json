{
  "name": "read-git",
  "description": "Read Git: An Ionic project",
  "homepage": "https://readgit.herokuapp.com/",
  "private": false,
  "scripts": {
    "cordova": "./node_modules/cordova/bin/cordova",
    "build": "ionic-app-scripts build",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build",
    "heroku-postbuild": "npm run build && gzip -k -r --best --force www",
    "livereload": "ionic run android --livereload --address 192.168.137.1 --device",
    "android": "cordova run android --buildConfig=.vscode/build.json",
    "release": "npm run build && npm run android -- --release",
    "deeplink": "adb shell am start -a android.intent.action.VIEW -d 'github://github.com/prijindal/readGit'"
  },
  "dependencies": {
    "@ionic/storage": "^1.0.3",
    "ionic-angular": "^2.0.0-rc.0",
    "ionic-native": "^2.0.3",
    "ionicons": "^3.0.0",
    "moment": "^2.15.1",
    "octokat": "^0.4.18",
    "x2js": "^3.0.0"
  },
  "devDependencies": {
    "cordova": "^6.3.1",
    "@ionic/app-scripts": "^0.0.23",
    "typescript": "^2.0.3"
  },
  "description": "testapp: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": []
}
