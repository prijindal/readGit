    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "es6-shim": "0.35.1",
    "ionic-angular": "2.0.0-beta.11",
    "ionic-native": "1.3.10",
    "ionicons": "3.0.0",
    "moment": "^2.15.1",
    "octokat": "git+https://github.com/prijindal/octokat.js.git",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.6",
    "x2js": "^3.0.0",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "cordova": "^6.3.1",
    "del": "2.2.0",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.5",
    "ionic-gulp-browserify-typescript": "2.0.0",
    "ionic-gulp-fonts-copy": "^1.0.0",
    "ionic-gulp-html-copy": "^1.0.0",
    "ionic-gulp-sass-build": "^1.0.0",
    "ionic-gulp-scripts-copy": "^2.0.0",
    "ionic-gulp-tslint": "^1.0.0",
    "node-static": "^0.7.8",
    "tslint-ionic-rules": "0.0.4",
    "run-sequence": "1.1.5"
  },
  "name": "read-git",
  "description": "Read Git: An Ionic project",
  "scripts": {
    "postinstall": "cd node_modules/octokat && npm install && npm run dist && cd ../..",
    "gulp": "node ./node_modules/gulp/bin/gulp.js",
    "cordova": "./node_modules/cordova/bin/cordova",
    "heroku-postbuild": "sh deploy.sh",
    "livereload": "ionic run android --livereload --address 192.168.137.1 --device",
    "build": "gulp build --release",
    "android": "cordova run android --buildConfig=.vscode/build.json",
    "release": "npm run build && npm run android -- --release",
    "deeplink": "adb shell am start -a android.intent.action.VIEW -d 'github://github.com/prijindal/readGit'"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": []
}
